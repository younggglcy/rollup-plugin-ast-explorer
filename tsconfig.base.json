{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "paths": {
      "rollup-plugin-ast-explorer": ["./src/*", "./dist/*"]
    },
    "strict": true,
    "declaration": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "exclude": ["dist", "node_modules"],
  "$schema": "https://json.schemastore.org/tsconfig"
}

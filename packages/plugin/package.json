{
  "name": "rollup-plugin-ast-explorer",
  "type": "module",
  "version": "0.0.0",
  "description": "Rollup plugin that captures and serves AST data",
  "author": {
    "name": "younggglcy",
    "email": "younggglcy@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/younggglcy/rollup-plugin-ast-explorer/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/younggglcy/rollup-plugin-ast-explorer.git",
    "directory": "packages/plugin"
  },
  "bugs": {
    "url": "https://github.com/younggglcy/rollup-plugin-ast-explorer/issues"
  },
  "keywords": [
    "rollup-plugin",
    "rollup ast",
    "ast explorer"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "!dist/**/*.map",
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "DEBUG=rollup-plugin-ast-explorer rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript -w",
    "build": "rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript"
  },
  "peerDependencies": {
    "rollup": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "rollup": {
      "optional": true
    }
  },
  "dependencies": {
    "@rollup-plugin-ast-explorer/shared": "workspace:*",
    "@rollup/pluginutils": "^5.3.0",
    "debug": "^4.4.3",
    "detect-port": "^2.1.0",
    "h3": "^1.15.4",
    "picocolors": "^1.1.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.13"
  }
}
